.PHONY: clean All

All:
	@echo "----------Building project:[ TriangularNumberSequence - Debug ]----------"
	@cd "TriangularNumberSequence" && "$(MAKE)" -f  "TriangularNumberSequence.mk"
clean:
	@echo "----------Cleaning project:[ TriangularNumberSequence - Debug ]----------"
	@cd "TriangularNumberSequence" && "$(MAKE)" -f  "TriangularNumberSequence.mk" clean
